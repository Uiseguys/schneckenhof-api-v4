[
    {
      "group": "admin",
      "permissions": [
        {
          "resource": "*",
          "methods": "*",
          "action": "allow"
        }
      ]
    },
    {
      "group": "user",
      "permissions": [
        {
          "resource": "*",
          "methods": ["POST", "PATCH"],
          "action": "deny"
        }
      ]
    }
  ]
  